{
    "name": "dewsign/nova-blog",
    "description": "Blogging plugin for Laravel Nova",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "m2de@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "maxfactor/support": "^2.3.0",
        "benjaminhirsch/nova-slug-field": "^1.0",
        "dewsign/nova-repeater-blocks": "^1.2.0",
        "epartment/nova-dependency-container": "^1.1",
        "silvanite/brandenburg": "^1.0",
        "dewsign/nova-field-sortable": "^0.2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Dewsign\\NovaBlog\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dewsign\\NovaBlog\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
